{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "ets-ogcapi-edr10#edrgeojson",
    "title": "EDR GeoJSON",
    "description": "Prototype Schema for Validating EDR GeoJSON in the EDR API ETS",
    "type": "object",
    "required": [ "type" , "links" ],
    "oneOf": [
        {
            "title": "FeatureCollection",
            "properties": {
                "type": { "enum": [ "FeatureCollection" ] }
            }
        },
        {
            "title": "Feature",
            "properties": {
                "type": { "enum": [ "Feature" ] }
            }
        }
  ]
}
